<p id="notice"><%= notice %></p>

<h1>Four Results</h1>

<table>
  <thead>
    <tr>
      <th>R1</th>
      <th>R2</th>
      <th>R3</th>
      <th>R4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @four_results.each do |four_result| %>
      <tr>
        <td><%= four_result.R1 %></td>
        <td><%= four_result.R2 %></td>
        <td><%= four_result.R3 %></td>
        <td><%= four_result.R4 %></td>
        <td><%= link_to 'Show', four_result %></td>
        <td><%= link_to 'Edit', edit_four_result_path(four_result) %></td>
        <td><%= link_to 'Destroy', four_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Four Result', new_four_result_path %>
